function city(obj) {
    for (let element of Object.keys(obj)) {
        console.log(`${element} -> ${obj[element]}`)
    }

}
city({
    name: "Sofia",
    area: 492,
    population: 1238438,
    country: "Bulgaria",
    postCode: "1000"
});
console.log(`\n --- \n`);
city({
    name: "Plovdiv",
    area: 389,
    population: 1162358,
    country: "Bulgaria",
    postCode: "4000"
});
