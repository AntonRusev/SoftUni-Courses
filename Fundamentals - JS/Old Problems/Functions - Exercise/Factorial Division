function factorialDivision(numOne, numTwo) {
    let factorialOne = 1;
    let factorialTwo = 1;

    for (let i = 1; i <= numOne; i++) {
        factorialOne *= i;
    }

    for (let y = 1; y <= numTwo; y++) {
        factorialTwo *= y;
    }
    console.log((factorialOne / factorialTwo).toFixed(2))
}
factorialDivision(5,
    2);
console.log(`\n --- \n`);
factorialDivision(6,
    2);
