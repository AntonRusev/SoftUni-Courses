function language (country) {
    switch(country) {
        case 'England':
        case 'USA':
            console.log('English');
            break;
        case 'Argentina':
        case 'Spain':
        case 'Mexico':
            console.log('Spanish');
            break;
        default: 
            console.log('unknown');
            break;
    }
}
language("USA");
console.log(`\n --- \n`);
language("Germany");
