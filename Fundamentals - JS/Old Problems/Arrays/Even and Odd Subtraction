function evenAndOddSubtraction(arr) {
    let sumEven = 0;
    let sumOdd = 0;

        for (let element of arr) {
            if (element % 2 === 0) {
                sumEven += element;
            } else {
                sumOdd += element;
            }
        }
        console.log(sumEven - sumOdd)

}
evenAndOddSubtraction([1,2,3,4,5,6]);
console.log(`\n --- \n`);
evenAndOddSubtraction([3,5,7,9]);
console.log(`\n --- \n`);
evenAndOddSubtraction([2,4,6,8,10]);
