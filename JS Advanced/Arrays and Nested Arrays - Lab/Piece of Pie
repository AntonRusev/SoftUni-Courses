function pieceOfPie(array, startPie, endPie) {
    let startIndex = array.indexOf(startPie);
    let endIndex = array.indexOf(endPie) + 1;

    let result = array.slice(startIndex, endIndex);
    return result;
}
pieceOfPie(['Pumpkin Pie',
'Key Lime Pie',
'Cherry Pie',
'Lemon Meringue Pie',
'Sugar Cream Pie'],
'Key Lime Pie',
'Lemon Meringue Pie');
console.log(`\n --- \n`);
pieceOfPie(['Apple Crisp',
'Mississippi Mud Pie',
'Pot Pie',
'Steak and Cheese Pie',
'Butter Chicken Pie',
'Smoked Fish Pie'],
'Pot Pie',
'Smoked Fish Pie');
