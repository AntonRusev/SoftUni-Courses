function negativeOrPositiveNumbers(array){
    let result = [];
    for (let number of array) {
        if (number < 0){
            result.unshift(number);
        } else {
            result.push(number);
        }
    }
    console.log(result.join('\n'))
}
negativeOrPositiveNumbers([7, -2, 8, 9]);
console.log(`\n --- \n`);
negativeOrPositiveNumbers([3, -2, 0, -1]);
