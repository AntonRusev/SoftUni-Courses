function biggestElement(matrix) {
    let biggestNum = matrix[0][0];
    for (let row of matrix) {
        for (let element of row) {
            if (element > biggestNum) {
                biggestNum = element;
            }
        }
    }
   return biggestNum;
}
biggestElement([[20, 50, 10],
    [8, 33,Â 145]]);
console.log(`\n --- \n`);
biggestElement([[3, 5, 7, 12],
    [-1, 4, 33, 2],
    [8, 3, 0, 4]]);
